{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["src", "App.tsx", "nativewind-env.d.ts"],
  "exclude": ["node_modules", "android", "ios"]
}
